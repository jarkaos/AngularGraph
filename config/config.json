{
  "$schema": "https://dev.office.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "angular-graph-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/angularGraph/AngularGraphWebPart.js",
          "manifest": "./src/webparts/angularGraph/AngularGraphWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "angular": {
      "path": "./node_modules/angular/angular.js",
      "globalName": "angular"
    },
    "ng-office-fabric-ui": {
      "path": "./node_modules/ng-office-ui-fabric/ngOfficeUiFabric.min.js",
      "globalName": "ng-office-fabric-ui",
      "globalDependencies": [
        "angular"
      ]
    },
    "hellojs": {
      "path": "./node_modules/hellojs/src/hello.js",
      "globalName": "hellojs"
    }
  },
  "localizedResources": {
    "AngularGraphWebPartStrings": "lib/webparts/angularGraph/loc/{locale}.js"
  }
}
